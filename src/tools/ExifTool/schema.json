{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "ExifTool": {
            "additionalProperties": false,
            "description": "Root object containing all EXIF metadata",
            "properties": {
                "ApertureValue": {
                    "description": "Aperture value",
                    "type": "number"
                },
                "Artist": {
                    "description": "Name of the image creator",
                    "type": "string"
                },
                "FocalPlaneYResolution": {
                    "description": "Focal plane Y resolution",
                    "type": "number"
                },
                "BrightnessValue": {
                    "description": "Brightness value",
                    "type": "number"
                },
                "ColorSpace": {
                    "description": "Color space",
                    "type": "string"
                },
                "Copyright": {
                    "description": "Copyright information",
                    "type": "string"
                },
                "CreateDate": {
                    "description": "Date and time when the image was created",
                    "type": "string"
                },
                "DateTimeOriginal": {
                    "description": "Original date and time when the image was taken",
                    "type": "string"
                },
                "Directory": {
                    "description": "Directory containing the image file",
                    "type": "string"
                },
                "ExifByteOrder": {
                    "description": "Byte order of the EXIF data",
                    "type": "string"
                },
                "ExifImageHeight": {
                    "description": "Height of the image in pixels",
                    "type": "number"
                },
                "ExifImageWidth": {
                    "description": "Width of the image in pixels",
                    "type": "number"
                },
                "ExifToolVersion": {
                    "description": "Version of ExifTool used to extract the metadata",
                    "type": "string"
                },
                "ExifVersion": {
                    "description": "EXIF version",
                    "type": "string"
                },
                "ExposureBiasValue": {
                    "description": "Exposure bias value",
                    "type": "number"
                },
                "ExposureCompensation": {
                    "description": "Exposure compensation value",
                    "type": "number"
                },
                "ExposureProgram": {
                    "description": "Exposure program used",
                    "type": "string"
                },
                "FocalPlaneXResolution": {
                    "description": "Focal plane X resolution",
                    "type": "number"
                },
                "ExposureTime": {
                    "description": "Exposure time (e.g., '1/10')",
                    "type": "string"
                },
                "FNumber": {
                    "description": "F-number (aperture)",
                    "type": "number"
                },
                "FileAccessDate": {
                    "description": "Date the file was last accessed",
                    "type": "string"
                },
                "FileInodeChangeDate": {
                    "description": "Date the file's inode was last changed",
                    "type": "string"
                },
                "FileModifyDate": {
                    "description": "Date the file was last modified",
                    "type": "string"
                },
                "FileName": {
                    "description": "Name of the image file",
                    "type": "string"
                },
                "FilePermissions": {
                    "description": "File permissions in octal format",
                    "type": "string"
                },
                "FileSize": {
                    "description": "Size of the file with unit (e.g., '5.0 MB')",
                    "type": "string"
                },
                "FileType": {
                    "description": "Type of image file (e.g., 'JPEG')",
                    "type": "string"
                },
                "FileTypeExtension": {
                    "description": "File extension (e.g., 'jpg')",
                    "type": "string"
                },
                "Flash": {
                    "description": "Flash status",
                    "type": "string"
                },
                "FocalLength": {
                    "description": "Focal length of the lens",
                    "type": "string"
                },
                "ISO": {
                    "description": "ISO speed rating",
                    "type": "number"
                },
                "ImageHeight": {
                    "description": "Height of the image in pixels",
                    "type": "number"
                },
                "ImageSize": {
                    "description": "Dimensions of the image (e.g., '5568x3712')",
                    "type": "string"
                },
                "ImageWidth": {
                    "description": "Width of the image in pixels",
                    "type": "number"
                },
                "LensModel": {
                    "description": "Model of the lens used",
                    "type": "string"
                },
                "LightSource": {
                    "description": "Light source",
                    "type": "string"
                },
                "MIMEType": {
                    "description": "MIME type of the file",
                    "type": "string"
                },
                "Make": {
                    "description": "Camera manufacturer",
                    "type": "string"
                },
                "MaxApertureValue": {
                    "description": "Maximum aperture value",
                    "type": "number"
                },
                "Megapixels": {
                    "description": "Size of the image in megapixels",
                    "type": "number"
                },
                "MeteringMode": {
                    "description": "Metering mode used",
                    "type": "string"
                },
                "Model": {
                    "description": "Camera model",
                    "type": "string"
                },
                "ModifyDate": {
                    "description": "Date the file was last modified",
                    "type": "string"
                },
                "Orientation": {
                    "description": "Orientation of the image",
                    "type": "number"
                },
                "ProfileCMMType": {
                    "description": "Type of color management module",
                    "type": "string"
                },
                "ProfileConnectionSpace": {
                    "description": "Connection space of the color profile",
                    "type": "string"
                },
                "ProfileDateTime": {
                    "description": "Date and time the profile was created",
                    "type": "string"
                },
                "ProfileFileSignature": {
                    "description": "File signature of the color profile",
                    "type": "string"
                },
                "ProfileVersion": {
                    "description": "Version of the color profile",
                    "type": "string"
                },
                "RecommendedExposureIndex": {
                    "description": "Recommended exposure index",
                    "type": "number"
                },
                "ResolutionUnit": {
                    "description": "Unit of resolution",
                    "type": "number"
                },
                "SensitivityType": {
                    "description": "Type of sensitivity",
                    "type": "number"
                },
                "ShutterSpeedValue": {
                    "description": "Shutter speed value",
                    "type": "number"
                },
                "Software": {
                    "description": "Software used to process the image",
                    "type": "string"
                },
                "SourceFile": {
                    "description": "Path to the source image file",
                    "type": "string"
                },
                "SubSecTime": {
                    "description": "Fractional seconds",
                    "type": "string"
                },
                "SubSecTimeDigitized": {
                    "description": "Fractional seconds of the digitized date and time",
                    "type": "string"
                },
                "SubSecTimeOriginal": {
                    "description": "Fractional seconds of the original date and time",
                    "type": "string"
                },
                "UprightCenterMode": {
                    "description": "Center mode of the upright transformation",
                    "type": "string"
                },
                "UprightFocalLength35mm": {
                    "description": "Focal length in 35mm format",
                    "type": "number"
                },
                "UprightFocalMode": {
                    "description": "Focal mode of the upright transformation",
                    "type": "string"
                },
                "UprightPreview": {
                    "description": "Preview of the upright transformation",
                    "type": "string"
                },
                "UprightTransformCount": {
                    "description": "Number of upright transformations",
                    "type": "number"
                },
                "UprightVersion": {
                    "description": "Version of the upright transformation",
                    "type": "string"
                },
                "UserComment": {
                    "description": "User comment",
                    "type": "string"
                },
                "XResolution": {
                    "description": "Horizontal resolution in DPI",
                    "type": "number"
                },
                "YCbCrSubSampling": {
                    "description": "Subsampling ratio of Y to C",
                    "type": "string"
                },
                "YResolution": {
                    "description": "Vertical resolution in DPI",
                    "type": "number"
                }
            },
            "type": "object"
        }
    },
    "required": [
        "ExifTool"
    ],
    "type": "object"
}
